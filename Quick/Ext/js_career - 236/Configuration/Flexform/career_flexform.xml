<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<sDEF>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.setting.general</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.storagePID>
						<TCEforms>
							<label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.storagePID</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>2</size>
								<autoSizeMax>30</autoSizeMax>
								<maxitems>99</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<default />
							</config>
						</TCEforms>
					</settings.storagePID>
				</el>
			</ROOT>
		</sDEF>
		<emailSetting>
		  <ROOT>
			<TCEforms>
			  <sheetTitle>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.emailConfiguration</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
			 <settings.subjectUser>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.subjectUser</label>
				  <config>
					<type>input</type>
					<size>48</size>
					<max>100</max>
					<default>Thanks you for your response. We will get back to you as soon as possible.</default>
				  </config>
				</TCEforms>
			  </settings.subjectUser>
			  <settings.subjectAdmin>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.subjectAdmin</label>
				  <config>
					<type>input</type>
					<default>A new user has applied for job</default>
					<size>48</size>
					<max>100</max>
				  </config>
				</TCEforms>
			  </settings.subjectAdmin>
			  <settings.senderName>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.senderName</label>
				  <config>
					<type>input</type>
					<size>30</size>
					<max>50</max>
					<default>Sender</default>
				  </config>
				</TCEforms>
			  </settings.senderName>
			  <settings.senderEmail>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.senderEmail</label>
				  <config>
					<type>input</type>
					<size>30</size>
					<max>50</max>
					<default>sender@sender.com</default>
				  </config>
				</TCEforms>
			  </settings.senderEmail>
			  <settings.noreply>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.noreply</label>
				  <config>
					<type>input</type>
					<size>30</size>
					<max>50</max>
					<default>noreply</default>
				  </config>
				</TCEforms>
			  </settings.noreply>
			  <settings.noreplyEmail>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.noreplyEmail</label>
				  <config>
					<type>input</type>
					<size>30</size>
					<max>250</max>
					<default>noreply@noreply.com</default>
				  </config>
				</TCEforms>
			  </settings.noreplyEmail>
			  <settings.sendMailUser>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.sendMailUser</label>
				  <config>
					  <type>check</type>
					  <default>1</default>
				  </config>
				</TCEforms>
			  </settings.sendMailUser>
			  <settings.sendMailAdmin>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.sendMailAdmin</label>
				  <config>
					  <type>check</type>
					  <default>1</default>
				  </config>
				</TCEforms>
			  </settings.sendMailAdmin>
			</el>
		  </ROOT>
		</emailSetting>
		<additional>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.setting.additional</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.limit>
						<TCEforms>
							<label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.limit</label>
							<config>
								<type>input</type>
								<size>5</size>
								<max>4</max>
								<eval>num</eval>
							</config>
						</TCEforms>
					</settings.limit>
					<settings.paginate>
						<TCEforms>
							<label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.paginate</label>
							<onChange>reload</onChange>
							<config>
								<type>check</type>
								<default>0</default>
							</config>
						</TCEforms>
					</settings.paginate>
					<settings.recordPerPage>
						<TCEforms>
							<label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.recordPerPage</label>
							<displayCond>FIELD:settings.paginate:=:1</displayCond>
							<config>
								<type>input</type>
								<size>2</size>
								<max>2</max>
								<default>10</default>
								<eval>num</eval>
							</config>
						</TCEforms>
					</settings.recordPerPage>
					<settings.listPage>
						<TCEforms>
							<label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.listPage</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<autoSizeMax>1</autoSizeMax>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<default />
							</config>
						</TCEforms>
					</settings.listPage>
					<settings.appliedJobLink>
						<TCEforms>
							<label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.appliedJobLink</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<autoSizeMax>1</autoSizeMax>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<default />
							</config>
						</TCEforms>
					</settings.appliedJobLink>
				</el>
			</ROOT>
		</additional>
		<label>
		  <ROOT>
			<TCEforms>
			  <sheetTitle>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.labels</sheetTitle>
			</TCEforms>
			<type>array</type>
			<el>
			 <settings.headerInsearchBox>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.headerInsearchBox</label>
				  <config>
					<type>input</type>
					<size>48</size>
					<max>1000</max>
					<default>Karrier</default>
				  </config>
				</TCEforms>
			  </settings.headerInsearchBox>
			  <settings.footerInDetailPage>
				<TCEforms>
				  <label>LLL:EXT:js_career/Resources/Private/Language/locallang_flexform.xlf:flexform.footerInDetailPage</label>
				  <config>
					<type>input</type>
					<default>Aktuell sind alle Assistenzarztstellen besetzt, gerne k√∂nnen Sie sich aber initiativ bewerben.</default>
					<size>48</size>
					<max>1000</max>
				  </config>
				</TCEforms>
			  </settings.footerInDetailPage>
			</el>
		  </ROOT>
		</label>
	</sheets>
</T3DataStructure>