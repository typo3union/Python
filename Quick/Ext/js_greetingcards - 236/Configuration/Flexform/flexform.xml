<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<sheets>
		<main>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:main</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.main.pid>
						<TCEforms>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:main.pid</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>2</size>
								<autoSizeMax>30</autoSizeMax>
								<maxitems>99</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<default />
							</config>
						</TCEforms>
					</settings.main.pid>
					<settings.main.storagePID>
						<TCEforms>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:main.storagePID</label>
							<config>
								<type>group</type>
								<internal_type>db</internal_type>
								<allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
								<default />
							</config>
						</TCEforms>
					</settings.main.storagePID>
				</el>
			</ROOT>
		</main>
		<receiver>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:receiver</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.flexform.receiver.name>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:receiver.name</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.flexform.receiver.name>
					<settings.flexform.receiver.email>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:receiver.email</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
							</config>
						</TCEforms>
					</settings.flexform.receiver.email>
					<settings.flexform.receiver.subject>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:receiver.subject</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<default>New greeting card</default>
							</config>
						</TCEforms>
					</settings.flexform.receiver.subject>
					<settings.flexform.receiver.senderName>
						<TCEforms>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:receiver.senderName</label>
							<config>
								<type>input</type>
								<size>30</size>
								<max>50</max>
								<default>Sender</default>
							</config>
						</TCEforms>
					</settings.flexform.receiver.senderName>
					<settings.flexform.receiver.senderEmail>
						<TCEforms>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:receiver.senderEmail</label>
							<config>
								<type>input</type>
								<size>30</size>
								<max>50</max>
								<default>sender@sender.com</default>
							</config>
						</TCEforms>
					</settings.flexform.receiver.senderEmail>
					<settings.flexform.receiver.replyToName>
						<TCEforms>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:receiver.replyToName</label>
							<config>
								<type>input</type>
								<size>30</size>
								<max>50</max>
								<default>noreply</default>
							</config>
						</TCEforms>
					</settings.flexform.receiver.replyToName>
					<settings.flexform.receiver.replyToEmail>
						<TCEforms>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:receiver.replyToEmail</label>
							<config>
								<type>input</type>
								<size>30</size>
								<max>250</max>
								<default>noreply@noreply.com</default>
							</config>
						</TCEforms>
					</settings.flexform.receiver.replyToEmail>
				</el>
			</ROOT>
		</receiver>
		<thanks>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:thanks</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.thanks.body>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:thanks.body</label>
							<config>
								<type>text</type>
								<default>Thanks for greetings! We will forward it soon!</default>
							</config>
							<defaultExtras>richtext[cut|copy|paste|formatblock|bold|italic|underline|left|center|right|orderedlist|unorderedlist|outdent|indent|link|image|line|chMode]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.thanks.body>
					<settings.thanks.redirect>
						<TCEforms>
							<exclude>1</exclude>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:thanks.redirect</label>
							<config>
								<type>input</type>
								<eval>trim</eval>
								<wizards type="array">
									<_PADDING type="integer">2</_PADDING>
									<link type="array">
										<type>popup</type>
										<title>Link</title>
										<icon>link_popup.gif</icon>
										<script>browse_links.php?mode=wizard</script>
										<JSopenParams>height=300,width=500,status=0,menubar=0,scrollbars=1</JSopenParams>
									</link>
								</wizards>
							</config>
						</TCEforms>
					</settings.thanks.redirect>
				</el>
			</ROOT>
		</thanks>
		<additional>
			<ROOT>
				<TCEforms>
					<sheetTitle>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:additional</sheetTitle>
				</TCEforms>
				<type>array</type>
				<el>
					<settings.additional.includeJSinFooter>
						<TCEforms>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:additional.includeJSinFooter</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.additional.includeJSinFooter>
					<settings.additional.includeCSSinFooter>
						<TCEforms>
							<label>LLL:EXT:js_greetingcards/Resources/Private/Language/flexform.xlf:additional.includeCSSinFooter</label>
							<config>
								<type>check</type>
								<default>1</default>
							</config>
						</TCEforms>
					</settings.additional.includeCSSinFooter>
				</el>
			</ROOT>
		</additional>
	</sheets>
</T3DataStructure>