<T3DataStructure>
  <meta>
	<langDisable>1</langDisable>
	<!--  <langChildren>1</langChildren>!-->
  </meta>
	<sheets>
		<sDEF>
			<ROOT>
			 <TCEforms>
            <sheetTitle>Karriere Settings </sheetTitle>
            </TCEforms>
			<type>array</type>
				<el>
					<settings.category>
						<TCEforms>
							<label>Select Category</label>
							<config>
								 <type>group</type>
								  <internal_type>db</internal_type>
								  <allowed>tx_karriere_domain_model_category</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							 </config>
						</TCEforms>
					</settings.category>
					
					<settings.backLink>
						<TCEforms>
							<label>Back Link</label>
							<config>
								 <type>group</type>
								  <internal_type>db</internal_type>
								  <allowed>pages</allowed>
								<size>1</size>
								<maxitems>1</maxitems>
								<minitems>0</minitems>
								<show_thumbs>1</show_thumbs>
							 </config>
						</TCEforms>
					</settings.backLink>
					
					<settings.categoryText>
						<TCEforms>
						<label>Category short description</label>		
							<config>
								<type>text</type>
								<cols>48</cols>
								<rows>5</rows>
							</config>
							<defaultExtras>richtext[*]:rte_transform[mode=ts_css]</defaultExtras>
						</TCEforms>
					</settings.categoryText>
					
					<settings.imagShow>
						<TCEforms>
							<label>Image show : List view</label>
							<config>
							  <type>check</type>
							</config> 
						</TCEforms>
					</settings.imagShow>
					


				</el>
			</ROOT>
		</sDEF>
	</sheets>
</T3DataStructure>

	

