<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xlf:flexforms_general.generalSetting</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    <settings.storagePID>
                        <TCEforms>
                            <label>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xlf:flexforms_general.storagePID</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>2</size>
                                <autoSizeMax>30</autoSizeMax>
                                <maxitems>99</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <default></default>
                            </config>
                        </TCEforms>
                    </settings.storagePID>
					<settings.categories>
						<TCEforms>
							<label>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xml:flexforms_general.categories</label>
							<config>
								<type>select</type>
								<foreign_table>tx_jsevent_domain_model_category</foreign_table>
								<foreign_table_where> AND (tx_jsevent_domain_model_category.sys_language_uid = 0 OR tx_jsevent_domain_model_category.l10n_parent = 0) ORDER BY tx_jsevent_domain_model_category.title</foreign_table_where>
								<size>10</size>
								<autoSizeMax>20</autoSizeMax>
								<minitems>0</minitems>
								<maxitems>99</maxitems>
							</config>
						</TCEforms>
					</settings.categories>
					<settings.limit>
						<TCEforms>
							<label>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xml:flexforms_general.limit</label>
							<config>
								<type>input</type>
								<size>2</size>
								<max>2</max>
                                <eval>num</eval>
							</config>
						</TCEforms>
					</settings.limit>
					<settings.paginate>
						<TCEforms>
							<label>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xml:flexforms_general.paginate</label>
							<onChange>reload</onChange>
							<config>
								<type>check</type>
                                <default>0</default>
							</config>
						</TCEforms>
					</settings.paginate>
					<settings.recordPerPage>
						<TCEforms>
							<label>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xml:flexforms_general.recordPerPage</label>
							<displayCond>FIELD:settings.paginate:=:1</displayCond>
							<config>
								<type>input</type>
								<size>2</size>
								<max>2</max>
								<default>10</default>
                                <eval>num</eval>
							</config>
						</TCEforms>
					</settings.recordPerPage>
                    <settings.listPage>
                        <TCEforms>
                            <label>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xlf:flexforms_general.listPage</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>1</size>
                                <autoSizeMax>30</autoSizeMax>
                                <maxitems>1</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                                <default></default>
                            </config>
                        </TCEforms>
                    </settings.listPage>
                </el>
            </ROOT>
        </sDEF>
		<template>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xlf:flexforms_general.template.setting</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
 					<settings.list>
						<TCEforms>
							<label>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xml:flexforms_general.template.list</label>
							<config>
								<type>input</type>
								<size>20</size>
								<max>50</max>
                                <eval>trim</eval>
								<default>List</default>
							</config>
						</TCEforms>
					</settings.list>
 					<settings.detail>
						<TCEforms>
							<label>LLL:EXT:js_event/Resources/Private/Language/locallang_flexform.xml:flexforms_general.template.detail</label>
							<config>
								<type>input</type>
								<size>20</size>
								<max>50</max>
                                <eval>trim</eval>
								<default>Detail</default>
							</config>
						</TCEforms>
					</settings.detail>
                </el>
            </ROOT>
        </template>
    </sheets>
</T3DataStructure>